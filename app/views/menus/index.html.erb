<p id="notice"><%= notice %></p>



<h1>Menus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Material</th>
      <th>Make</th>
      <th>Point</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @menus.each do |menu| %>
      <tr>
        <td><%= menu.name %></td>
        <td><%= menu.material %></td>
        <td><%= menu.make %></td>
        <td><%= menu.point %></td>
        <td><%= link_to 'Show', menu_path(menu) %></td>
        <td><%= link_to 'Edit', edit_menu_path(menu) %></td>
        <td><%= link_to 'Destroy', menu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

